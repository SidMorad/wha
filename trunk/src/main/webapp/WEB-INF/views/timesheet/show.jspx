<div xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fmt="http://java.sun.com/jsp/jstl/fmt" xmlns:form="http://www.springframework.org/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" version="2.0">
    <jsp:output omit-xml-declaration="yes"/>
    <script type="text/javascript">dojo.require('dijit.TitlePane');</script>
    <div id="_title_div">
        <spring:message code="label.timesheet" var="entity_label"/>
        <spring:message arguments="${entity_label}" code="entity.show" var="title_msg"/>
        <script type="text/javascript">Spring.addDecoration(new Spring.ElementDecoration({elementId : '_title_div', widgetType : 'dijit.TitlePane', widgetAttrs : {title: '${title_msg}'}})); </script>
        <c:if test="${not empty timesheet}">
            <div id="roo_timesheet_sheetYear">
                <label for="_sheetYear_id">Sheet Year:</label>
                <div class="box" id="_sheetYear_id">${timesheet.sheetYear}</div>
            </div>
            <br/>
            <div id="roo_timesheet_sheetMonth">
                <label for="_sheetMonth_id">Sheet Month:</label>
                <div class="box" id="_sheetMonth_id">${timesheet.sheetMonth}</div>
            </div>
            <br/>
            <div id="roo_timesheet_monthlyTotal">
                <label for="_monthlyTotal_id">Monthly Total:</label>
                <div class="box" id="_monthlyTotal_id">${timesheet.monthlyTotal}</div>
            </div>
            <br/>
            <div id="roo_timesheet_employee">
                <label for="_employee_id">Employee:</label>
                <div class="box" id="_employee_id">${timesheet.employee}</div>
            </div>
            <br/>
            <div id="roo_timesheet_dailyTimesheets">
                <label for="_dailyTimesheets_id">Daily Timesheets:</label>
                <div class="box" id="_dailyTimesheets_id">${timesheet.dailyTimesheets}</div>
            </div>
            <br/>
        </c:if>
        <c:if test="${empty timesheet}">
            <spring:message arguments="${entity_label}" code="entity.not.found.single"/>
        </c:if>
    </div>
</div>
